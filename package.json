{
    "name": "vscode-smart-split-into-lines",
    "displayName": "vscode-smart-split-into-lines",
    "description": "Split cursor by selected lines by Ctrl+Alt+l.",
    "version": "0.0.1",
    "publisher": "JulioGold",
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:smart.splitIntoLines"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "smart.splitIntoLines",
            "title": "Split into lines"
        }],
        "keybindings": [
			{ "key": "ctrl+alt+l",       "when": "editorTextFocus", "command": "smart.splitIntoLines" }
		]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}